<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".User_Management.feedback_package.fragment_allmyFeedback">

    <!-- TODO: Update blank fragment layout -->
   <android.support.v7.widget.RecyclerView

       android:id="@+id/myAllFeedback_recyclerView"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:visibility="gone"
       >

   </android.support.v7.widget.RecyclerView>
   <com.github.ybq.android.spinkit.SpinKitView
       android:id="@+id/myAllFeedback_progressBar"
       android:layout_width="100sp"
       android:layout_height="100sp"
       android:layout_gravity="center"
       style="@style/SpinKitView.Small.Wave"
       app:SpinKit_Color="@color/colorPrimary"
       android:visibility="visible"
       />

    <include
        android:visibility="gone"
        android:id="@+id/no_ratings_layout"
        layout="@layout/no_ratings"
        />

</FrameLayout>